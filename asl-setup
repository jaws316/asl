#!/bin/bash
cd .bin
wget https://raw.githubusercontent.com/jaws316/asl/main/dodropin-c
chmod 755 dodropin-c
wget https://raw.githubusercontent.com/jaws316/asl/main/dodropin-d
chmod 755 dodropin-d
wget https://raw.githubusercontent.com/jaws316/asl/main/gb3dq-c
chmod 755 gb3dq-c
wget https://raw.githubusercontent.com/jaws316/asl/main/gb3dq-d
chmod 755 gb3dq-d
wget https://raw.githubusercontent.com/jaws316/asl/main/gb3fh-c
chmod 755 gb3fh-c
wget https://raw.githubusercontent.com/jaws316/asl/main/gb3fh-d
chmod 755 gb3fh-d
wget https://raw.githubusercontent.com/jaws316/asl/main/mx0onl-c
chmod 755 mx0onl-c
wget https://raw.githubusercontent.com/jaws316/asl/main/mx0onl-d
chmod 755 mx0onl-d
cd /etc/asterisk/local/
wget https://raw.githubusercontent.com/jaws316/asl/main/condis.sh
chmod 755 condis.sh
cd
